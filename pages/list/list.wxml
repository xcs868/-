    <text>幼苗龙虾</text>
  </view>
  <view class="index_anli">
    <view>
      <view wx:for="{{imgalist1}}" wx:for-item="image" class="index_anli_v" data-type="非标轴承">
        <image src="{{image}}" data-src="{{image}}" bindtap="previewImage"></image>
        <text>幼苗龙虾</text>
      </view>
    </view>
  </view>
