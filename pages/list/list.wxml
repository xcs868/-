      <view wx:for="{{imgalist2}}" wx:for-item="image" class="index_anli_v" data-type="轨道滑轮轴承">
        <image src="{{image}}" data-src="{{image}}" bindtap="previewImage"></image>
        <text>极品大虾</text>
      </view>
    </view>
  </view>
  <!------------------------------------------------------------------->
  <view class="text">
    <view class="line_y"></view>
    <text>鲜嫩龙虾</text>
  </view>
